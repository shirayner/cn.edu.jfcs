<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.edu.jfcs.model">
	<class
		name="YearTeachUnit"
		table="YEARTEACHUNIT"
		dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="identity"/>
		</id>

		<property
			name="nian"
			column="NIAN"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="unitid"
			column="UNITID"
			type="string"
			not-null="true"
			length="8"
		/>			
		<property
			name="haszyk"
			column="HASZYK"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="hasggk"
			column="HASGGK"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="ssb"
			column="SSB"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="jfys"
			column="JFYS"
			type="big_decimal"
			not-null="true"
			length="10"
		/>
		<property
			name="jfss"
			column="JFSS"
			type="big_decimal"
			not-null="true"
			length="10"
		/>
		<property
			name="jxywper"
			column="JXYWPER"
			type="big_decimal"
			not-null="true"
			length="5"
		/>
		<property
			name="jxglper"
			column="JXGLPER"
			type="big_decimal"
			not-null="true"
			length="5"
		/>
		<property
			name="jxyjper"
			column="JXYJPER"
			type="big_decimal"
			not-null="true"
			length="5"
		/>
		<property
			name="szpyper"
			column="SZPYPER"
			type="big_decimal"
			not-null="true"
			length="5"
		/>
		<property
			name="ta1"
			column="TA1"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta2"
			column="TA2"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta3"
			column="TA3"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta4"
			column="TA4"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta5"
			column="TA5"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta6"
			column="TA6"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta7"
			column="TA7"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="ta8"
			column="TA8"
			type="integer"
			not-null="true"
			length="10"
		/>
		<many-to-one
			name="teachunit"
			class="cn.edu.jfcs.model.TeachUnit"
			update="false"
			insert="false"
			lazy="false"
			column="unitid"
		/>
	</class>	
</hibernate-mapping>